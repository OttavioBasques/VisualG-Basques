Algoritmo "DistingValor"

Var
   num1,num2: real

Inicio
   escreva("Informe o primeiro valor: ")
   leia(num1)
   escreva("Informe o segundo valor: ")
   leia(num2)
   se(num1>num2) entao
      escreva("O maior valor é: ", num1)
   senao
      escreva("O maior valor é: ", num2)
   fimse


Fimalgoritmo